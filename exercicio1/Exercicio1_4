public class Exercicio1_4{
	public static void main(String [] args){
		float a=45,b=20,c=3;
		float soma=a+b+c,mult=a*b*c,sub=a-b-c;
		float div=a/b/c; 
		System.out.println("O valor da soma foi: " + soma);
		System.out.println("O valor da multiplicacao foi: " + mult);
		System.out.println("O valor da divisao foi: " + div );
		System.out.println("O valor da subtracao foi: " + sub );
	}
}
